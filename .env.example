# AWS Configuration for E-commerce Application
VITE_AWS_REGION=us-east-2
VITE_AWS_ACCESS_KEY_ID=your-access-key-id
VITE_AWS_SECRET_ACCESS_KEY=your-secret-access-key
VITE_AWS_USER_POOL_ID=your-user-pool-id
VITE_AWS_USER_POOL_CLIENT_ID=your-client-id
VITE_AWS_USER_POOL_CLIENT_SECRET=your-client-secret
VITE_AWS_IDENTITY_POOL_ID=your-identity-pool-id
VITE_AWS_TOKEN_SIGNING_URL=https://cognito-idp.your-region.amazonaws.com/your-user-pool/.well-known/jwks.json
VITE_DYNAMODB_TABLE_NAME=your-products-table
VITE_DYNAMODB_PARTITION_KEY=products_id

# S3 Configuration for Image Storage
VITE_S3_BUCKET_NAME=your-bucket-name
VITE_S3_REGION=us-east-2

# Real-time Synchronization Configuration
VITE_SYNC_POLLING_INTERVAL=10000
VITE_ENABLE_REAL_TIME_SYNC=true

# Application Configuration
VITE_APP_NAME=Sibiling Shoes Store
VITE_APP_VERSION=1.0.0
VITE_API_BASE_URL=https://your-api-id.execute-api.region.amazonaws.com/dev

# Development Configuration
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug

# Feature Flags
VITE_ENABLE_GOOGLE_SIGNIN=false
VITE_ENABLE_SOCIAL_LOGIN=false
VITE_ENABLE_GUEST_CHECKOUT=true